{
  "permissions": {
    "allow": [
      "Bash(dir:*)",
      "Bash(tree:*)",
      "Bash(npm install:*)",
      "Bash(timeout 3 node:*)",
      "Bash(curl:*)",
      "Bash(for file in *.js)",
      "Bash(do mv \"$file\" \"$file%.js.cjs\")",
      "Bash(done)",
      "Bash(for file in *.cjs)",
      "Bash(do mv \"$file\" \"$file%.cjs.js\")"
    ]
  }
}
